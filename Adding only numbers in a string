#include <bits/stdc++.h>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    string str;
	    cin>>str;
	    int ans=0;
	    for(int i=0;i<str.length();i++){
	        if(isdigit(str[i]))
	        ans+=(str[i]-48);
	    }
	    cout<<ans<<endl;
	}
	return 0;
}
----------------------------Pyhton solution---------------------------------
for _ in range(int(input())):
    sm=0
    num="1234567890"
    s=input()
    for i in s:
        if i in num:
            sm+=int(i)
    print(sm)
