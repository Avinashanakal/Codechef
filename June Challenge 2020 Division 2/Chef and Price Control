#include <bits/stdc++.h>
#define lli long long int
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--){
        int n,k;
        cin>>n>>k;
        int arr[n];
        int ans=0;
        for(int i=0;i<n;++i){
            cin>>arr[i];
        }
        for(int i=0;i<n;++i){
            if(arr[i]>k){
                ans+=arr[i]-k;
            }
        }
        cout<<ans<<"\n";
    }
	return 0;
}
