#include <iostream>
#include<algorithm>
using namespace std;

int main() {
    long long t;
    cin>>t;
    while(t--)
    {
        long long n;
        cin>>n;
        long long a[n];
        for(int i=0;i<n;i++)
        {
            cin>>a[i];
        }
        long long tot=n*(n-1)/2;
        cout<<tot<<endl;
    }
	return 0;
}
